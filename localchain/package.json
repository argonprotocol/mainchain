{
  "name": "@ulixee/localchain",
  "version": "2.0.0-alpha.28",
  "bin": "cli.js",
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --no-const-enum --features=napi",
    "build:debug": "shx rm -rf target && napi build --platform --no-const-enum --features=fast-runtime,napi -p ulx-localchain",
    "prepublishOnly": "napi prepublish -t npm",
    "pretest": "npm run build:debug",
    "test": "npm run pretest && jest",
    "universal": "napi universal",
    "version": "napi version"
  },
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "localchain",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "universal-apple-darwin"
    ]
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0-alpha.44",
    "@polkadot/util-crypto": "^12.6.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.6",
    "@types/pg": "^8.10.9",
    "@ulixee/mainchain": "2.0.0-alpha.28",
    "jest": "^29.7.0",
    "nanoid": "^3.3.7",
    "pg": "^8.11.3",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">= 18"
  },
  "packageManager": "yarn@4.1.0"
}
