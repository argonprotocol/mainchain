{
  "references": [
    {
      "path": "localchain/tsconfig-cjs.json"
    },
    {
      "path": "client/nodejs/tsconfig-cjs.json"
    }
  ],
  "compilerOptions": {
    "target": "es2022",
    "module": "CommonJS",
    "declaration": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "types": ["node", "jest"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": true
  },
  "include": []
}
